{
  "presets": ["es2015", "react", "stage-2"],
  "env": {
    "dev": {
      "plugins": ["react-hot-loader"]
    },
    "test": {
      "plugins": ["rewire"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "transformations": "./melinda-ui-commons/server/record-transformations/",
        "commons": "./melinda-ui-commons/frontend/js/",
      }
    }]
  ]
}